{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties":
	{
        "eventDate" : {
          "description": "TODO: Get content off of Emily",
          "$ref": "#/definitions/ISO8601-Date"
        },
        "lifeEventType" : {
          "description": "TODO: TBC by Emily",
          "type" : "string",
          "enum": ["Purchase extension 1", "Purchase extension 2"]
        },
        "conveyancerReference": {
          "description": "TODO: TBC by Emily",
          "type": "string",
          "pattern": "^[a-zA-Z0-9 :/-]{1,20}$",
          "example": "CR12345-6789"
        },
        "purchaseFundReleaseLifeEventId": {
          "description": "Life event id for the release of funds that is to be extended.",
          "$ref": "#/definitions/IDType"
        },
        "supersededLifeEventId": {
          "description": "Life event id for the event that is being superseded.",
          "$ref": "#/definitions/IDType"
        },
        "supersededLifeEventDate": {
          "description": "Life event date for the event that is being superseded.",
          "$ref": "#/definitions/ISO8601-Date"
        }
	},
	"required": [
		"eventDate",
		"withdrawalAmount"
    ],
	"additionalProperties": false,
	"definitions":
	{
      "ISO8601-Date": {
        "id": "full-date",
        "type": "string",
        "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
        "example": "2017-05-06"
      },
      "IDType": {
        "type": "string",
        "pattern": "^\\d{10}$",
        "example": "0987654321"
      }
	}
}
